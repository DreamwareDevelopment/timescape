{
  "private": true,
  "name": "demo",
  "version": "1.2.1",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "concurrently -n 'integration,storybook' 'pnpm vite --port 4949' 'pnpm storybook:dev --no-open'",
    "dev:integrations": "vite build && concurrently 'pnpm storybook:build' 'vite build -l error -w'",
    "build": "pnpm typecheck && vite build && pnpm storybook:build",
    "preview": "vite preview",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build -o storybook-dist",
    "storybook:test": "storybook test"
  },
  "dependencies": {
    "@preact/signals": "1.2.3",
    "@rehookify/datepicker": "6.6.1",
    "htm": "^3.1.1",
    "preact": "^10.22.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "solid-js": "^1.8.17",
    "svelte": "^4.2.17",
    "timescape": "workspace:*",
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "7.6.5",
    "@storybook/addon-controls": "7.6.5",
    "@storybook/addon-essentials": "^7.6.5",
    "@storybook/addon-interactions": "^7.6.5",
    "@storybook/addon-links": "^7.6.5",
    "@storybook/blocks": "^7.6.5",
    "@storybook/manager-api": "^7.6.5",
    "@storybook/react": "^7.6.5",
    "@storybook/react-vite": "^7.6.5",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@types/node": "20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@vanilla-extract/css": "^1.15.1",
    "@vanilla-extract/vite-plugin": "^4.0.9",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "concurrently": "^8.2.2",
    "storybook": "^7.6.5",
    "storybook-addon-code-editor": "2.2.0",
    "vite": "^5.2.11",
    "vite-plugin-top-level-await": "1.4.1"
  }
}
