{
  "private": true,
  "name": "demo",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "pnpm storybook",
    "dev:integrations": "vite build && concurrently 'pnpm storybook' 'vite build -l error -w'",
    "build": "pnpm typecheck && vite build && pnpm build-storybook",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o storybook-dist",
    "test-storybook": "storybook test"
  },
  "dependencies": {
    "@preact/signals": "1.1.3",
    "htm": "3.1.1",
    "preact": "10.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "solid-js": "1.7.6",
    "svelte": "3.59.1",
    "timescape": "workspace:*",
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "7.4.0",
    "@storybook/addon-controls": "7.4.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/blocks": "^7.4.0",
    "@storybook/jest": "0.2.2",
    "@storybook/manager-api": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/react-vite": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@sveltejs/vite-plugin-svelte": "2.4.5",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vanilla-extract/css": "1.13.0",
    "@vanilla-extract/vite-plugin": "3.9.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitejs/plugin-vue": "4.3.4",
    "concurrently": "8.2.1",
    "prop-types": "^15.8.1",
    "storybook": "^7.4.0",
    "vite": "^4.4.9"
  }
}
