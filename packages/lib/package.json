{
  "name": "timescape",
  "license": "MIT",
  "version": "0.1.9",
  "description": "A flexible, headless date and time input library for JavaScript. Provides tools for building fully customizable date and time input fields, with support for libraries like React, Preact, Vue, Svelte and Solid.",
  "repository": "https://github.com/dan-lee/timescape",
  "bugs": "https://github.com/dan-lee/timescape/issues",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "build": "tsup",
    "prepublishOnly": "pnpm test run && pnpm typecheck && pnpm build && cp ../../README.md . && cp ../../LICENSE ."
  },
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*.js"
    },
    "./chunk-*": null
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@preact/signals": "1.1.3",
    "@testing-library/dom": "9.3.0",
    "@testing-library/jest-dom": "5.16.5",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/testing-library__jest-dom": "5.14.6",
    "@vitest/coverage-v8": "0.32.0",
    "happy-dom": "9.20.3",
    "preact": "10.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "solid-js": "1.7.6",
    "svelte": "3.59.1",
    "tsup": "^6.7.0",
    "vitest": "0.32.0",
    "vue": "3.3.4"
  },
  "peerDependencies": {
    "@preact/signals": "1.x",
    "preact": "10.x",
    "react": ">=17",
    "react-dom": ">=17",
    "solid-js": ">=1",
    "svelte": "3.x",
    "vue": "3.x"
  },
  "peerDependenciesMeta": {
    "@preact/signals": {
      "optional": true
    },
    "preact": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "solid-js": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "keywords": [
    "date",
    "time",
    "datetime",
    "input",
    "headless",
    "a11y",
    "customizable",
    "flexible",
    "date-time-management",
    "typescript",
    "vanilla-js",
    "react",
    "preact",
    "vue",
    "svelte",
    "solid"
  ]
}
